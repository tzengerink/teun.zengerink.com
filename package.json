{
  "name": "teun.zengerink.com",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "next build && next export",
    "dev": "next dev",
    "lint:ts": "eslint src/**/*.{ts,tsx}",
    "lint:types": "tsc --noEmit",
    "lint": "yarn lint:ts && yarn lint:types",
    "postinstall": "husky install && playwright install",
    "start": "next start",
    "test:ci": "yarn lint && jest --ci --collectCoverage && yarn test:e2e",
    "test:e2e": "playwright test src/e2e",
    "test:smoke": "export $(grep -v '^#' .env | xargs) && curl -v --silent $NEXT_PUBLIC_PROTO://$NEXT_PUBLIC_HOST/ 2>&1 | grep -q $(git rev-parse HEAD)",
    "test": "jest --watch"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "js-yaml": "^4.1.0",
    "marked": "^4.0.12",
    "next": "^12.0.10",
    "next-build-id": "^3.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "sass": "^1.49.7",
    "sharp": "^0.30.0",
    "slugify": "^1.6.5"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@playwright/test": "^1.18.1",
    "@testing-library/react": "^12.1.2",
    "@types/image-size": "^0.8.0",
    "@types/jest": "^27.4.0",
    "@types/marked": "^4.0.2",
    "@types/node": "^17.0.14",
    "@types/react": "^17.0.39",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.4.2",
    "babel-jest": "^27.4.6",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.0",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.18",
    "typescript": "^4.5.5"
  }
}
