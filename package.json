{
  "name": "teun.zengerink.com",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "next build && next export",
    "dev": "next dev",
    "lint:ts": "eslint src/",
    "lint:types": "tsc --noEmit",
    "lint": "yarn lint:ts && yarn lint:types",
    "postinstall": "husky install && playwright install",
    "start": "next start",
    "test:ci": "yarn lint && jest --ci --collectCoverage && yarn test:e2e",
    "test:e2e": "playwright test src/e2e",
    "test:smoke": "export $(grep -v '^#' .env | xargs) && curl -v --silent $NEXT_PUBLIC_PROTO://$NEXT_PUBLIC_HOST/ 2>&1 | grep -q $(git rev-parse HEAD)",
    "test": "jest --watch"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "js-yaml": "^4.1.0",
    "marked": "^4.0.17",
    "next": "^12.1.6",
    "next-build-id": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.30.6",
    "slugify": "^1.6.5"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@playwright/test": "^1.22.2",
    "@testing-library/react": "^13.3.0",
    "@types/image-size": "^0.8.0",
    "@types/jest": "^28.1.1",
    "@types/marked": "^4.0.3",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "autoprefixer": "^10.4.7",
    "babel-jest": "^28.1.1",
    "eslint": "^7.32.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "postcss": "^8.4.14",
    "prettier": "^2.7.0",
    "tailwindcss": "^3.1.3",
    "typescript": "^4.7.3"
  }
}
